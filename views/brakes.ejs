<!DOCTYPE html>
<html lang="en">
	<%- include('partials/head') %>

	<body>
		<%- include('partials/header') %>
		<h2><%= title %></h2>
		<table>
			<thead>
				<tr>
					<th>Name</th>
					<th>Price ($)</th>
					<th>Stock Quantity</th>
					<th>Description</th>
				</tr>
			</thead>
			<tbody>
				<% brakeParts.forEach(item => { %>
				<tr>
					<td><%= item.name %></td>
					<td><%= item.price %></td>
					<td><%= item.stock_qty %></td>
					<td><%= item.description %></td>
				</tr>
				<% }) %>
			</tbody>
		</table>
	</body>
</html>
