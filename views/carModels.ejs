<!DOCTYPE html>
<html lang="en">
	<%- include('partials/head') %>
	<body>
		<%- include('partials/header') %>
		<table border="1" cellspacing="0" cellpadding="8">
			<thead>
				<tr>
					<!-- <th>ID</th> -->
					<th>Make</th>
					<th>Model</th>
					<th>Year</th>
				</tr>
			</thead>
			<tbody>
				<% cars.forEach((car) => { %>
				<tr>
					<!-- <td><%= car.id %></td> -->
					<td><%= car.make %></td>
					<td><%= car.model %></td>
					<td><%= car.year %></td>
				</tr>
				<% }); %>
			</tbody>
		</table>
	</body>
</html>
