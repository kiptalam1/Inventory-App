<!DOCTYPE html>
<html lang="en">
	<%- include('partials/head') %>
	<body>
		<%- include('partials/header') %>
		<h1>Available Parts</h1>

		<table border="1">
			<thead>
				<tr>
					<th>Name</th>
					<th>Description</th>
					<th>Price</th>
					<th>Stock Quantity</th>
				</tr>
			</thead>
			<tbody>
				<% parts.forEach(function(part) { %>
				<tr>
					<td><%= part.name %></td>
					<td><%= part.description %></td>
					<td><%= part.price %></td>
					<td><%= part.stock_qty %></td>
				</tr>
				<% }); %>
			</tbody>
		</table>
	</body>
</html>
